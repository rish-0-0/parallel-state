{
  "name": "parallel-state",
  "version": "1.0.0",
  "description": "Parallel State: runs in a worker, different state model",
  "main": "lib/parallel-state.js",
  "module": "es/parallel-state.js",
  "unpkg": "dist/parallel-state.js",
  "types": "types/index.d.ts",
  "files": [
    "dist",
    "lib",
    "src",
    "es",
    "types"
  ],
  "scripts": {
    "clean": "rimraf lib dist es coverage types",
    "format": "prettier --write \"{src,tests}/**/*.{js,ts}\" \"**/*.md\"",
    "format:check": "prettier --list-different \"{src,tests}/**/*.{js,ts}\" \"**/*.md\"",
    "check-types": "tsc --noEmit",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "test:cov": "npm test -- --coverage",
    "build": "rollup -c",
    "pretest": "npm run build",
    "prepublish": "npm run clean && npm run check-types && npm run format:check && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rish-0-0/parallel-state.git"
  },
  "keywords": [
    "Worker",
    "State"
  ],
  "author": "Rishabh Anand",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rish-0-0/parallel-state/issues"
  },
  "homepage": "https://github.com/rish-0-0/parallel-state#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.10",
    "@babel/node": "^7.13.10",
    "@babel/plugin-external-helpers": "^7.12.13",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-flow": "^7.12.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/register": "^7.13.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-node-resolve": "^11.2.0",
    "@rollup/plugin-replace": "^2.4.1",
    "@types/jest": "^26.0.20",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.40.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "npmName": "parallel-state",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "dependencies": {
    "@babel/runtime": "^7.13.10",
    "comlink": "^4.3.0"
  }
}
